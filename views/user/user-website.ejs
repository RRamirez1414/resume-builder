<!DOCTYPE html>

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Sample Resume</title>
    <script
    src="https://code.jquery.com/jquery-3.6.0.js"
    integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
    crossorigin="anonymous"></script>
    <script
      src="https://code.jquery.com/jquery-3.6.0.min.js"
      integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-Piv4xVNRyMGpqkS2by6br4gNJ7DXjqk09RmUpJ8jgGtD7zP9yug3goQfGII0yAns"
      crossorigin="anonymous"
    ></script>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css"
      integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l"
      crossorigin="anonymous"
    />
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script src="https://kit.fontawesome.com/b3237b8abf.js" crossorigin="anonymous"></script>
    <% if (resumeInfo.theme != null && resumeInfo.theme == "dark") { %>
        <link rel="stylesheet" type="text/css" href="/DarkTheme.css" />
    <% } else if (resumeInfo.theme != null && resumeInfo.theme == "nmsu") { %>
        <link rel="stylesheet" type="text/css" href="/NMSUStyle.css" />
    <% } else { %>
        <link rel="stylesheet" type="text/css" href="/styles.css" />
    <% } %> 
     
    
    

</head>

<header>
    <div class="container-fluid header-box">
        <div class="mt-auto mr-auto"><h4><%= user.firstName + " " + user.lastName + " Resume"%></h4></div>
    </div>
</header>

<body>
    <div class="container mt-5 section-design w-50">
      <div class="mr-auto headers"><h1><%= resumeInfo.siteTitle %></h1></div>
      <div class="row">
        <div class="ml-4">
        <% if (resumeInfo.phone1 != null || resumeInfo.phone2 != null) { %>
          <div class=" ml-3 mr-auto mb-0 mt-2 row headers">Phone Numbers:</div>
            <% if (resumeInfo.phone1 != null) { %>
                <div class="mr-auto mt-2 row personal-info">
                    <div class="ml-5 mt-0 mr-1">
                    <% if (resumeInfo.phone1Type != null) { %>
                        <%= resumeInfo.phone1Type %>: 
                    <% } else { %>
                        Phone:
                    <% } %> 
                    </div>
                    <div class=" mr-1"><%= resumeInfo.phone1 %></div>
                </div>
            <% } %>
            <% if (resumeInfo.phone2 != null) { %>
                <div class="mr-auto row personal-info">
                    <div class="ml-5 mt-0 mr-1">
                    <% if (resumeInfo.phone2Type != null) { %>
                        <%= resumeInfo.phone2Type %>: 
                    <% } else { %>
                        Phone:
                    <% } %> 
                    </div> 
                    <div class=" mr-1"><%= resumeInfo.phone1 %></div>
                </div>
            <% } %>
         <% } %>  
        </div>
        <div class="ml-5">
            <% if (resumeInfo.email1 != null || resumeInfo.email2 != null) { %>
                <div class=" ml-3 mr-auto mt-2 row headers">Email:</div>
                <% if (resumeInfo.email1 != null) { %>
                    <div class=" ml-4 mr-auto personal-info">
                        <%= resumeInfo.email1 %> 
                    </div>
                <% } %>
                <% if (resumeInfo.email2 != null) { %>
                    <div class=" ml-4 mr-auto personal-info">
                        <%= resumeInfo.email2 %> 
                    </div>
                <% } %>
            <% } %>
        </div>
        <div class="ml-5">
            <% if (resumeInfo.link1 != null || resumeInfo.link2 != null) { %>
                <div class=" ml-3 mr-auto mt-2 row headers">Links:</div>
                <% if (resumeInfo.link1 != null) { %>
                    <div class=" ml-4 mr-auto personal-info">
                        <a href="//<%= resumeInfo.link1 %>" target="_blank"><%= resumeInfo.link1 %> </a>
                    </div>
                <% } %>
                <% if (resumeInfo.link2 != null) { %>
                    <div class=" ml-4 mr-auto personal-info">
                        <a href="//<%= resumeInfo.link2 %>" target="_blank"><%= resumeInfo.link2 %> </a>
                    </div>
                <% } %>
            <% } %>
        </div>
      </div>
    </div>

    
    <% if (resumeInfo.profSum != null) { %>
        <div class="container mt-4 section-design w-50">
            <div class="ml-1 mr-auto headers"><h3>About:</h3></div>
            <div class="ml-2 mr-auto">
              <%= resumeInfo.profSum %> 
            </div>
        </div>
    <% } %>

    <% if (resumeInfo.skills != null && resumeInfo.skills != "") { %>
        <div class="container mt-4 section-design w-50">
            <div class="ml-1 mr-auto headers"><h3>Skills:</h3></div>
            <div class="ml-2 mr-auto">
                <ul class="list-horizontal">
                    <% skills.forEach(skill => { %>
                        <li><%= skill %> </li>
                    <% }) %>
                </ul>
              </div>
        </div>
    <% } %>
   
    <% if (expInfo.length != 0) { %>
        <div class="container mt-4 section-design w-50">
            <div class="ml-1 mr-auto headers"><h3>Work Experience</h3>
                <% expInfo.forEach(exp => { %>
                    <% if (exp.expName != null) { %>
                        <div class="row">
                            <div class="ml-4 headers"><h4>Employer:</h4></div>
                            <div class=" ml-2 mr-auto"><h4><%= exp.expName %> </h4></div>
                        </div>
                    <% } %>
                    <% if (exp.title != null) { %>
                        <div class="row">
                            <div class="ml-4 headers">Title:</div>
                            <div class=" ml-2 mr-auto"><%= exp.title %></div>
                        </div>
                    <% } %>
                    <% if (exp.description != null) { %>
                        <div class="ml-2 mr-auto headers">Description:</div>
                        <div class="ml-3 mr-auto">
                            <%= exp.description %>
                        </div>
                    <% } %>
                    <% if (exp.jobDuties != null) { %>
                        <div class="ml-2 mr-auto headers">Duties:</div>
                        <div class="ml-3 mr-auto">
                            <%= exp.jobDuties %>
                        </div>
                    <% } %>
                    <% if (exp.fromMonth != null && exp.fromYear != null) { %>
                        <div class="row">
                            <div class="ml-4 headers">Start:</div>
                            <div class="ml-2 mr-4">
                            <%= exp.fromMonth + " " + exp.fromYear %> 
                        </div>
                    <% } %>
                    <% if (exp.toMonth != null && exp.toYear != null) { %>
                        <div class="ml-4 headers">End:</div>
                            <div class="ml-2 mr-auto">
                                <%= exp.toMonth + " " + exp.toYear %>
                            </div>
                        </div>
                    <% } %>
                    <% if (exp.city != null && exp.state != null) { %>
                        <div class="row">
                            <div class="ml-4 headers">Location: </div> 
                            <div class="ml-2 mr-auto"><%= exp.city + ", " + exp.state %> </div> 
                        </div>
                    <% } %>
                <% }) %>
            </div>
        </div>
    <% } %>
    
    <% if (eduInfo.length != 0) { %>
        <div class="container mt-4 section-design w-50">
            <div class="ml-1 mr-auto headers"><h3>Education</h3>
                <% eduInfo.forEach(edu => { %>
                    <% if (edu.institution != null) { %>
                        <div class="row">
                            <div class="ml-4 headers"><h4>Institution:</h4></div>
                            <div class=" ml-2 mr-auto">
                              <h4><%= edu.institution %> </h4>
                            </div>
                        </div>
                    <% } %>
                    <% if (edu.achieved != null && edu.program != null) { %>
                        <div class="row">
                            <div class="ml-4 headers">Degree:</div>
                            <div class=" ml-2 mr-auto">
                              <%= edu.achieved + " " +  edu.program %> 
                            </div>
                        </div>
                    <% } %>
                    <% if (edu.gradYear != null && edu.graduated != null) { %>
                        <div class="row">
                            <div class="ml-4 headers">Graduation Year:</div>
                            <div class="ml-2 mr-4">
                              <%= edu.gradYear %> 
                            </div>
                            <div class="ml-4 headers">Graduated:</div>
                            <div class="ml-2 mr-auto">
                                <% if (edu.gradYear == 'Y') { %>
                                    Yes
                                <% } else { %>
                                    No
                                <% } %> 
                            </div>
                        </div>
                    <% } %>
                    <% if (edu.city != null || edu.state != null) { %>
                        <div class="row">
                            <div class="ml-4 headers">Location: </div> 
                            <div class="ml-2 mr-auto"><%= edu.city + ", " + edu.state %></div> 
                        </div>
                    <% } %>
                <% }) %>
      
          </div>
        </div>
    <% } %>
</body>

<footer>

  <div class="container-fluid header-box mt-5">
    <div class="mt-auto mr-auto"><h4>Web-Res</h4></div>
  </div>

</footer>