<%- include("../partials/header.ejs") %>

<div class="interface-design w-50 mr-auto ml-auto mt-5">
    <div class="mr-auto text-center mb-2"><h1>Resume List</h1></div>
    <hr class="sep mb-2">
    <% resumeList.forEach(resume => { %>
    <div class="w-50 justify-center text-center">
        <div class="mr-3"><h3><%= resume.siteTitle %></h3></div>
        <div class="ml-auto"><h3><a class="mr-auto" href="view-list/<%= resume._id %>">Edit</a></h3></div>
    </div>
</div>

<% }); %> <%- include("../partials/footer.ejs") %>
